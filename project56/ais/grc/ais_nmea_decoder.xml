<?xml version="1.0"?>
<block>
  <name>AIS-NMEA-DecodeToSQL</name>
  <key>ais_nmea_decoder</key>
  <category>AIS</category>
  <import>import ais</import>
  <make>ais.nmea_decoder($host,$database,$user,$password)</make>
  <param>
     <name>SQL hostname</name>
     <key>host</key>
     <type>string</type>
  </param>
  <param>
     <name>SQL database</name>
     <key>database</key>
     <type>string</type>
  </param>
  <param>
     <name>SQL user</name>
     <key>user</key>
     <type>string</type>
  </param>
  <param>
     <name>SQL password</name>
     <key>password</key>
     <type>string</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
    <vlen>200</vlen>
  </sink>

</block>
